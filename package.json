{
  "name": "@1inch/limit-order-sdk",
  "version": "4.0.1-rc.0",
  "description": "SDK for 1inch Limit Order Protocol",
  "author": "@1inch",
  "license": "MIT",
  "private": false,
  "exports": {
    ".": "./dist/index.js",
    "./axios": "./dist/axios-ext.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:1inch/limit-order-sdk.git"
  },
  "scripts": {
    "build": "tsc",
    "docs:generate": "tsdoc --src=src/limit-order/* --dest=src/limit-order/README.md",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint:ci": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint": "eslint  \"{src,apps,libs,test}/**/*.ts\" --ignore-path .eslintignore --fix",
    "lint:types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "changelog:generate": "changelog generate -a"
  },
  "dependencies": {
    "@1inch/byte-utils": "1.0.1",
    "@metamask/eth-sig-util": "^5.0.2",
    "ethers": "6.11.0"
  },
  "devDependencies": {
    "@1inch/eslint-config": "1.4.3",
    "@1inch/tsconfig": "1.0.7",
    "@swc/core": "1.3.66",
    "@swc/jest": "0.2.26",
    "@types/jest": "29.5.2",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "5.59",
    "@typescript-eslint/parser": "5.51",
    "eslint": "8.41.0",
    "eslint-config-prettier": "8.3",
    "eslint-config-standard": "17",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.26",
    "eslint-plugin-n": "16",
    "eslint-plugin-prettier": "4",
    "eslint-plugin-promise": "6",
    "eslint-plugin-unused-imports": "2",
    "generate-changelog": "^1.8.0",
    "jest": "29.5.0",
    "prettier": "^2.3.0",
    "tsdoc-markdown": "0.3.0",
    "typescript": "4.9.4",
    "axios": "^1.6.7"
  },
  "peerDependencies": {
    "axios": ">=1 <2",
    "assert": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "axios": {
      "optional": true
    },
    "assert": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "volta": {
    "node": "18.16.0"
  }
}
